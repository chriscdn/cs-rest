{
  "name": "@kweli/cs-rest",
  "version": "3.0.0",
  "description": "Simple authentication and REST calls for OpenText Content Server.",
  "type": "module",
  "source": "./src/index.ts",
  "exports": {
    "types": "./dist/index.d.ts",
    "require": "./dist/cs-rest.cjs",
    "default": "./dist/cs-rest.modern.js"
  },
  "main": "./dist/cs-rest.cjs",
  "module": "./dist/cs-rest.esm.js",
  "unpkg": "./dist/cs-rest.umd.js",
  "types": "./dist/index.d.ts",
  "repository": "https://github.com/chriscdn/cs-rest",
  "author": "Christopher Meyer <chris.meyer@kwe.li> (https://kwe.li/)",
  "license": "MIT",
  "scripts": {
    "build____": "rm -rf ./dist/ && microbundle --globals lodash.get=get,lodash.isnil=isnil ",
    "build": "rm -rf ./dist/ && microbundle --format modern,esm,cjs",
    "watch": "microbundle watch",
    "bump": "versiony package.json --patch"
  },
  "dependencies": {
    "axios": "^1.6.8",
    "form-data": "^4.0.0",
    "is-object": "^1.0.2",
    "lodash.get": "^4.4.2",
    "lodash.isnil": "^4.0.0"
  },
  "devDependencies": {
    "@types/lodash.get": "^4.4.9",
    "@types/lodash.isnil": "^4.0.9",
    "@types/node": "^20.12.7",
    "esbuild": "^0.20.2",
    "microbundle": "^0.15.1",
    "openapi-typescript": "^6.7.5",
    "typescript": "^5.4.5",
    "versiony": "^2.0.1"
  },
  "keywords": [
    "opentext",
    "content-server",
    "rest",
    "axios"
  ]
}
